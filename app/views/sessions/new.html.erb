<% provide(:title, "Log in") %>
<h1>Log in</h1>

<div>
  <div>
    <%= form_for(:session, url: login_path) do |f| %>

      <p><%= f.label :email %></p>
      <p><%= f.email_field :email %></p>

      <p><%= f.label :password %></p>
      <p><%= f.password_field :password %></p>

      <%= f.submit "Log in" %>
    <% end %>

  </div>
</div>



